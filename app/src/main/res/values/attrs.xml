<resources>
    <!-- Specifies a style resource to use for an indeterminate progress spinner. -->
    <attr name="indeterminateDSAProgressStyle" format="reference" />
    <item name="TAG_VIEW_ID" type="id" />
    <item name="TAG_TITLE_ID" type="id" />

    <item name="dynamic_view_address" type="id" />

    <item name="level0" type="id" />
    <item name="level1" type="id" />
    <item name="level2" type="id" />
    <item name="level3" type="id" />
    <item name="level4" type="id" />
    <item name="level5" type="id" />
    <item name="level6" type="id" />
    <item name="level7" type="id" />

    <declare-styleable name="HeaderFrameLayout">
        <attr name="layout_mode" format="enum">
            <enum name="topLeft" value="0" />
            <enum name="topRight" value="1" />
            <enum name="bottomLeft" value="2" />
            <enum name="bottomRight" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CasoView">
        <attr name="cardTitle" format="string" />
        <attr name="scoreColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ShoppingCart">
        <attr name="cartTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="Account360Header">
        <attr name="customImageAspectRatio" format="fraction" />
    </declare-styleable>

    <declare-styleable name="DiagonalLineView">
        <attr name="lineSize" format="dimension" />
        <attr name="lineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PerformanceIndicatorsView">
        <attr name="sectionTitle" format="string" />
    </declare-styleable>

</resources>